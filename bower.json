{
  "name": "snapcar-sdk-js",
  "version": "0.1.0",
  "homepage": "https://github.com/snapcar/snapcar-sdk-js",
  "description": "The SnapCar SDK for JavaScript allows communication between your Web application and the SnapCar platform. It allows your application to perform actions such as making bookings on behalf of your user.",
  "main": [
    "dist/SnapCar.min.js"
  ],
  "dependencies": {
    "jquery": ">=1.5"
  },
  "keywords": [
    "snapcar",
    "chauffeur",
    "chauffeurs",
    "driver",
    "drivers",
    "booking",
    "bookings",
    "vtc",
    "transport",
    "travel"
  ],
  "authors": [
    {
      "name": "Jonathan Tiret",
      "email": "jonathan@snapcar.com"
    }
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ]
}